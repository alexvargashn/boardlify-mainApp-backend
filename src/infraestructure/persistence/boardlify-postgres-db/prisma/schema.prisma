generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  user_id  String @id @default(cuid()) @db.VarChar
  name     String @db.VarChar
  username String @db.VarChar @unique
  email    String @db.VarChar @unique
  password String @db.VarChar
}
